        <label rdf:datatype="http://www.project-best.eu/owl/airm-mod/meteorology.owl#PlainLiteral">FEW</label>
    </owl:NamedIndividual>
    


    <!-- http://www.project-best.eu/owl/airm-mod/meteorology.owl#CodeCloudAmountType-OVERCAST -->

