        <label rdf:datatype="http://www.project-best.eu/owl/airm-mod/meteorology.owl#PlainLiteral">OVERCAST</label>
    </owl:NamedIndividual>
    


    <!-- http://www.project-best.eu/owl/airm-mod/meteorology.owl#CodeCloudAmountType-SCATTERED -->

    <owl:NamedIndividual rdf:about="http://www.project-best.eu/owl/airm-mod/meteorology.owl#CodeCloudAmountType-SCATTERED">
        <rdf:type rdf:resource="http://www.project-best.eu/owl/airm-mod/meteorology.owl#CodeCloudAmountType"/>
        <comment rdf:datatype="http://www.project-best.eu/owl/airm-mod/meteorology.owl#PlainLiteral">(3 to 4 oktas)</comment>
        <label rdf:datatype="http://www.project-best.eu/owl/airm-mod/meteorology.owl#PlainLiteral">SCATTERED</label>
    </owl:NamedIndividual>
    


    <!-- http://www.project-best.eu/owl/airm-mod/meteorology.owl#CodeCloudAmountType-SCATTERED_BROKEN -->

    <owl:NamedIndividual rdf:about="http://www.project-best.eu/owl/airm-mod/meteorology.owl#CodeCloudAmountType-SCATTERED_BROKEN">
