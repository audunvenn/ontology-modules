        <rdfs:subClassOf rdf:resource="http://www.project-best.eu/owl/airm-mod/meteorology.owl#_Meteorology_"/>
        <comment rdf:datatype="http://www.project-best.eu/owl/airm-mod/meteorology.owl#PlainLiteral">Local variation of the wind vector or any of its components in a given direction.</comment>
    </owl:Class>
    


