        <label rdf:datatype="http://www.project-best.eu/owl/airm-mod/meteorology.owl#PlainLiteral">BROKEN</label>
    </owl:NamedIndividual>
    


    <!-- http://www.project-best.eu/owl/airm-mod/meteorology.owl#CodeCloudAmountType-FEW -->

    <owl:NamedIndividual rdf:about="http://www.project-best.eu/owl/airm-mod/meteorology.owl#CodeCloudAmountType-FEW">
